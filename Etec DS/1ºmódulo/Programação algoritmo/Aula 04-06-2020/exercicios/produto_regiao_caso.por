programa
{
	
	funcao inicio()
	{
		real preco
		inteiro codigo

		escreva("Infome o preço do produto \n ")
		leia(preco)
		escreva("Informe o codigo de região \n ")
		leia(codigo)

		escolha(codigo){

			caso 1: escreva("O produto custa: ",preco," e o produto veio do SUL")
			pare
			caso 2: escreva("O produto custa: ",preco," e o produto veio do NORTE")
			pare
			caso 3: escreva("O produto custa: ",preco," e o produto veio do LESTE")
			pare
			caso 4: escreva("O produto custa: ",preco," e o produto veio do OESTE")
			pare
			caso 5: caso 6: caso 25:caso 26:caso 27:caso 28:caso 29:caso 30:  escreva("O produto custa: ",preco," e o produto veio do NORDESTE")
			pare
			caso 7: caso 8: caso 9:  escreva("O produto custa: ",preco," e o produto veio do SUDESTE")
			pare
			caso 10:caso 11:caso 12:caso 13:caso 14:caso 15:caso 16:caso 17:caso 18:caso 19: caso 20:  escreva("O produto custa: ",preco," e o produto veio do CENTRO-OESTE")
			pare
			caso contrario :escreva ("Esse produto é importado")
		}
	}
}
